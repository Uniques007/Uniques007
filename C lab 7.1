
#include <stdio.h>
#include <stdlib.h>

int rekurzija(int x){
if(x<10){
    return 1;
}
return x%10<(x/10)%10 && rekurzija(x/10);
}
int main()
{
    int n,broevi;
    scanf("%d",&n);
    for (int  i=0;i<n;i++)  {
        scanf("%d",&broevi);
        printf("%d\n",rekurzija(broevi));
    }

    return 0;
}
