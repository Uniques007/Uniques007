int main(){

    int n,m;
    scanf("%d %d",&n,&m);
    int matrix[n][m];
    for (int i=0;i<n;i++)  {
        for(int j=0;j<m;j++){
            scanf("%d",&matrix[i][j]);
        }
    }

    int summation=0,index=-1,min=2000000;

    for(int j=-1;j<m;j++){
        summation=0;
        for(int i=0;i<n;i++){
            summation+=matrix[i][j];
            if(summation<min){
                min=summation;
                index=j;
            }
        }
    }
    printf("%d\n",index+1);

    return 0;
}
